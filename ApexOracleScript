----------------------------------------
--Dropping Tables
----------------------------------------

DROP TABLE Generalblood CASCADE CONSTRAINT;
DROP TABLE Donormedhistory CASCADE CONSTRAINT;
DROP TABLE Donorblood CASCADE CONSTRAINT;
DROP TABLE Donor CASCADE CONSTRAINT;
DROP TABLE Donationevent CASCADE CONSTRAINT;
DROP TABLE Bloodtest CASCADE CONSTRAINT;
DROP TABLE Hospital CASCADE CONSTRAINT;
DROP TABLE Records CASCADE CONSTRAINT; 
DROP TABLE Appointment CASCADE CONSTRAINT;
DROP TABLE Bloodbank CASCADE CONSTRAINT;
DROP TABLE Receipt CASCADE CONSTRAINT;
DROP TABLE Recipient CASCADE CONSTRAINT;
DROP TABLE Recipientblood CASCADE CONSTRAINT;
DROP TABLE Inventory CASCADE CONSTRAINT;

----------------------------------------
--Creating Tables
----------------------------------------

CREATE TABLE Donorblood
(DonorID NUMBER(3) NOT NULL,
BloodTypeD CHAR(5),
ExpirationDate DATE,
StorageCondition VARCHAR2(15),
PRIMARY KEY (BloodTypeD, DonorID));

CREATE TABLE Generalblood
(EventID NUMBER(3) NOT NULL,
BloodTypeGen CHAR(4),
BloodTypeD CHAR(5),
BloodTypeR CHAR(5),
PRIMARY KEY (EventID, BloodTypeGen, BloodTypeD));

CREATE TABLE Donormedhistory
(DonorID NUMBER(3) NOT NULL,
DonorMedHistory VARCHAR2(100),
PRIMARY KEY (DonorID));

CREATE TABLE Donor
(DonorID NUMBER(3) NOT NULL,
DonorName VARCHAR2(100),
DonorContact NUMBER(12),
NextOfKinD NUMBER(12),
BloodTypeGen CHAR(4),
PRIMARY KEY (DonorID));

CREATE TABLE Donationevent
(EventID NUMBER(3) NOT NULL,
DonorID NUMBER(3) NOT NULL,
AppID NUMBER(3)NOT NULL,
ExpirationDate DATE,
EventDate DATE,
PRIMARY KEY (EventID, DonorID));

CREATE TABLE Bloodtest
(DonorID NUMBER(3) NOT NULL,
TestResults VARCHAR2(100),
TestDate DATE,
CONSTRAINT TestResultsCheck CHECK (TestResults IN ('Eligible', 'Ineligible')),
PRIMARY KEY (DonorID));

CREATE TABLE Hospital
(HospitalID NUMBER(3) NOT NULL,
HospitalName CLOB,
HospitalLocation VARCHAR2(100),
HospitalPhoneLine NUMBER(12),
BuiltDate DATE,
PRIMARY KEY (HospitalID));

CREATE TABLE Records
(RecordID NUMBER(3) NOT NULL,
BloodTypeD CHAR(5),
DonorMedHistory VARCHAR2(100),
BloodTypeGen VARCHAR2(4),
RecipientID NUMBER(3) NOT NULL,
CONSTRAINT RecordIdCheck CHECK (RecordID BETWEEN 400 and 421),
PRIMARY KEY (RecordID));

CREATE TABLE Appointment
(AppID NUMBER(3) NOT NULL,
AppDate DATE,
AppTime INTERVAL DAY TO SECOND, 
HospitalID NUMBER(3),
PRIMARY KEY (AppID));

CREATE TABLE Bloodbank
(BloodBankID NUMBER(3) NOT NULL,
BloodBankName VARCHAR2(100),
StorageCap NUMBER(3),
HospitalID NUMBER(3) NOT NULL,
PRIMARY KEY (BloodBankID));

CREATE TABLE Receipt
(ReceiptID NUMBER(3) NOT NULL,
AppID NUMBER(3) NOT NULL,
CONSTRAINT ReceiptIDCheck CHECK (ReceiptID BETWEEN 600 and 621),
PRIMARY KEY (ReceiptID));

CREATE TABLE Recipient
(RecipientID NUMBER(3) NOT NULL,
RecipientName VARCHAR2(100),
NextOfKinR NUMBER(12),
UrgencyLevel NUMBER(1),
BloodTypeR CHAR(5),
CONSTRAINT CheckUrgencyLevel CHECK (UrgencyLevel IN ('1', '2', '3', '4', '5')),
PRIMARY KEY (RecipientID));

CREATE TABLE Recipientblood
(BloodTypeR CHAR(5),
RecipientID NUMBER(3) NOT NULL,
StorageCondition VARCHAR2(15),
ExpirationDate DATE,
CONSTRAINT StorageConditionCheck CHECK (StorageCondition IN ('Well', 'Excellent')),
PRIMARY KEY (BloodTypeR, RecipientID, ExpirationDate));

CREATE TABLE Inventory
(HospitalID NUMBER(3) NOT NULL,
BloodBankID NUMBER(3) NOT NULL,
Quantity NUMBER(3),
PRIMARY KEY (HospitalID, BloodBankID));

-----------------------------------------------------------------------------------------------------
--Creating Indexes
--We do this so that these two tables can be easily accessed and retrieved when running a command
-----------------------------------------------------------------------------------------------------

CREATE INDEX DonorProfile
 on Donor (DonorID, DonorName, DonorContact, BloodtypeGen, NextOfKinD);

 CREATE INDEX RecipientProfile
 on Recipient (RecipientID, RecipientName, BloodTypeR, NextOfKinR);

--------------------------------------------------------------------------------
--Creating Views
--------------------------------------------------------------------------------

CREATE OR REPLACE FORCE EDITIONABLE VIEW "DONORVIEW" ("DONORID", "DONORNAME", "DONORCONTACT", "NEXTOFKIND", "BLOODTYPED", "EVENTDATE", "EXPIRATIONDATE", "HOSPITALNAME", "HOSPITALLOCATION", "HOSPITALPHONELINE") AS 
  select DONOR.DONORID as DONORID,
    DONOR.DONORNAME as DONORNAME,
    DONOR.DONORCONTACT as DONORCONTACT,
    DONOR.NEXTOFKIND as NEXTOFKIND,
    DONORBLOOD.BLOODTYPED as BLOODTYPED,
    DONATIONEVENT.EVENTDATE as EVENTDATE,
    DONATIONEVENT.EXPIRATIONDATE as EXPIRATIONDATE,
    HOSPITAL.HOSPITALNAME as HOSPITALNAME,
    HOSPITAL.HOSPITALLOCATION as HOSPITALLOCATION,
    HOSPITAL.HOSPITALPHONELINE as HOSPITALPHONELINE 
 from DONATIONEVENT DONATIONEVENT,
    APPOINTMENT APPOINTMENT,
    HOSPITAL HOSPITAL,
    DONORBLOOD DONORBLOOD,
    DONOR DONOR 
 where DONOR.DONORID=DONORBLOOD.DONORID
    and DONOR.DONORID=DONATIONEVENT.DONORID
    and APPOINTMENT.APPID=DONATIONEVENT.APPID
    and HOSPITAL.HOSPITALID=APPOINTMENT.HOSPITALID
 ORDER BY DonorID;

CREATE OR REPLACE FORCE EDITIONABLE VIEW "RECEPIENTVIEW" ("RECIPIENTID", "RECIPIENTNAME", "NEXTOFKINR", "URGENCYLEVEL", "BLOODTYPER", "EXPIRATIONDATE") AS 
  select RECIPIENT.RECIPIENTID as RECIPIENTID,
    RECIPIENT.RECIPIENTNAME as RECIPIENTNAME,
    RECIPIENT.NEXTOFKINR as NEXTOFKINR,
    RECIPIENT.URGENCYLEVEL as URGENCYLEVEL,
    RECIPIENTBLOOD.BLOODTYPER as BLOODTYPER,
    RECIPIENTBLOOD.EXPIRATIONDATE as EXPIRATIONDATE 
 from RECIPIENTBLOOD RECIPIENTBLOOD,
    RECIPIENT RECIPIENT 
 where RECIPIENT.RECIPIENTID=RECIPIENTBLOOD.RECIPIENTID
 ORDER BY RecipientID;

